<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :url %>
    <%= f.input @project.pictures.image %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
